                                                    <Grid Grid.Column="0" Grid.Row="1">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <Border Grid.Row="0" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <Border Grid.Row="1" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <TextBlock Grid.Row="0" Margin="3" Text="2 min"></TextBlock>
                                                        <TextBlock Grid.Row="1" Margin="3" Text="{Binding Const2m, StringFormat=0.00}"></TextBlock>
                                                    </Grid>
                                                    <Grid Grid.Column="1" Grid.Row="1">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <Border Grid.Row="0" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <Border Grid.Row="1" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <TextBlock Grid.Row="0" Margin="3" Text="4 min"></TextBlock>
                                                        <TextBlock Grid.Row="1" Margin="3" Text="{Binding Const4m, StringFormat=0.00}"></TextBlock>
                                                    </Grid>
                                                    <Grid Grid.Column="2" Grid.Row="1">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <Border Grid.Row="0" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <Border Grid.Row="1" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <TextBlock Grid.Row="0" Margin="3" Text="5 min"></TextBlock>
                                                        <TextBlock Grid.Row="1" Margin="3" Text="{Binding Const5m, StringFormat=0.00}"></TextBlock>
                                                    </Grid>
                                                    <Grid Grid.Column="3" Grid.Row="1">
