                    <StackPanel Orientation="Horizontal">
                        <Image Source="/jpg/Battary.png" Width="15" Height="15"/>
                        <TextBlock Margin="3">Перечень АКБ</TextBlock>
                    </StackPanel>
                </TabItem.Header>
                <TabItem.Content>
                    <ScrollViewer>
                        <Border Background="White" BorderBrush="Gainsboro" BorderThickness="3">
                            <StackPanel>
                                <DataGrid ItemsSource="{Binding BattaryList2}" x:Name="phonesGrid" AutoGenerateColumns="False" HorizontalGridLinesBrush="DarkGray"
                                            RowBackground="LightGray" AlternatingRowBackground="White">
                                    <DataGrid.Columns>
                                        <DataGridTextColumn IsReadOnly="True" Header="ID" Binding="{Binding ID}"></DataGridTextColumn>
                                        <DataGridTextColumn IsReadOnly="True" Header="Арт. Legrand" Binding="{Binding LegArt}" Width="100"></DataGridTextColumn>
                                        <DataGridTextColumn IsReadOnly="True" Header="Арт. Сторонний" Binding="{Binding OtherArt}"></DataGridTextColumn>
                                        <DataGridTextColumn IsReadOnly="True" Header="Тип" Binding="{Binding Mark}" Width="80"></DataGridTextColumn>
