                                                    </Grid>
                                                    <Grid Grid.Column="4" Grid.Row="1">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <Border Grid.Row="0" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <Border Grid.Row="1" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <TextBlock Grid.Row="0" Margin="3" Text="8 min"></TextBlock>
                                                        <TextBlock Grid.Row="1" Margin="3" Text="{Binding Const8m, StringFormat=0.00}"></TextBlock>
                                                    </Grid>
                                                    <Grid Grid.Column="5" Grid.Row="1">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition></RowDefinition>
                                                            <RowDefinition></RowDefinition>
                                                        </Grid.RowDefinitions>
                                                        <Border Grid.Row="0" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <Border Grid.Row="1" BorderThickness="1" BorderBrush="Black" Background="White"></Border>
                                                        <TextBlock Grid.Row="0" Margin="3" Text="10 min"></TextBlock>
